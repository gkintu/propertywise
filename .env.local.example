# Gemini API Configuration
GEMINI_API_KEY=

# Site Configuration
SITE_URL=http://localhost:3000
APP_TITLE=PropertyAI

# Feature Flags Configuration
# Set these to 'true' to enable features, 'false' or leave empty to disable

# Property search functionality
NEXT_PUBLIC_ENABLE_PROPERTY_SEARCH=true

# Recent analysis section
NEXT_PUBLIC_ENABLE_RECENT_ANALYSIS=true

# Replace with your actual Upstash Redis REST URL and token

UPSTASH_REDIS_REST_URL= "https://us1-xxxxxx.upstash.io"
UPSTASH_REDIS_REST_TOKEN="your_token_here"